+@INSTALL_SUDO_SCRIPTS_TRUE@install-data-hook:
+@INSTALL_SUDO_SCRIPTS_TRUE@	$(MKDIR_P) $(DESTDIR)/$(sysconfdir)/sudoers.d
+@INSTALL_SUDO_SCRIPTS_TRUE@	$(INSTALL) -m 440 sudoers.ovirt-guest-agent $(DESTDIR)/$(sysconfdir)/sudoers.d/50_ovirt-guest-agent
