+@%:@ Check whether --with-sudohelper was given.
+if test "${with_sudohelper+set}" = set; then :
+  withval=$with_sudohelper; sudohelper=${withval}
+else
+  sudohelper=usermode
+fi
+
+
+
+case $sudohelper in
+    usermode)
+        ;;
+    sudo)
+        ;;
+    *)
+        as_fn_error $? "sudohelper must be one of usermode or sudo" "$LINENO" 5
+        ;;
+esac
+
+ if test -n "$sudohelper" -a "$sudohelper" == "usermode"; then
+  INSTALL_USERMODE_SCRIPTS_TRUE=
+  INSTALL_USERMODE_SCRIPTS_FALSE='#'
+else
+  INSTALL_USERMODE_SCRIPTS_TRUE='#'
+  INSTALL_USERMODE_SCRIPTS_FALSE=
+fi
