+
+@%:@ Check whether --with-sso was given.
+if test "${with_sso+set}" = set; then :
+  withval=$with_sso; sso=${withval}
+else
+  sso=yes
+fi
+
+
+
+@%:@ Check whether --with-dist was given.
+if test "${with_dist+set}" = set; then :
+  withval=$with_dist; dist=${withval}
+else
+  dist=no
+fi
+
+
+
+@%:@ Check whether --with-gdm was given.
+if test "${with_gdm+set}" = set; then :
+  withval=$with_gdm; gdm=${withval}
+else
+  gdm=$sso
+fi
+
+
+
+@%:@ Check whether --with-kdm was given.
+if test "${with_kdm+set}" = set; then :
+  withval=$with_kdm; kdm=${withval}
+else
+  kdm=$sso
+fi
